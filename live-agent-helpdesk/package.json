  "description": "To make it easy for you to get started with GitLab, here's a list of recommended next steps.",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prod": "NODE_ENV=production webpack --mode production --config webpack-config.js && nodemon server.js",
    "uat": "NODE_ENV=production webpack --mode production --config webpack-config.js && node server.js",
    "stage": "NODE_ENV=production webpack --mode production --config webpack-config.js && VERIFY_USER=No node server.js",
    "dev": " NODE_ENV=development webpack --mode development --config webpack-config.js && node server.js",
    "dev4": "NODE_ENV=development webpack --mode development --config webpack-config.js && node server.js",
    "local": "cross-env NODE_ENV=development API_END_POINT=local webpack-dev-server --mode development --config webpack-config.js && API_END_POINT=local VERIFY_USER=No nodemon server.js",
    "localServer": "NODE_ENV=development API_END_POINT=local webpack --mode production --config webpack-config.js && API_END_POINT=local VERIFY_USER=No  nodemon server.js",
    "server": "NODE_ENV=development API_END_POINT=local VERIFY_USER=No nodemon server.js",
    "watch": "npm run dev -- --watch"
  },
